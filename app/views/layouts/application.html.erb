<!DOCTYPE html>
<html>
<head>
  <title>roomER</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="navbar navbar-fixed-top navbar-inverse">
		<div class="navbar-inner">
				<ul>
					<li class="brand pull-left"><%= link_to "roomER", root_path %></li>
				</ul>
				<ul class="nav pull-right">
					<% if user_signed_in? %>
						Logged in as <strong><%= current_user.email %></strong>.
                        <%= link_to "Add Room", new_room_path, :class => 'navbar-link'  %> |
                        <%= link_to 'Create User', new_user_path, :class => 'navbar-link' %> |
                        <!-- <%= link_to "Create User", new_user_registration_path, :class => 'navbar-link'  %> | -->
                        <%= link_to "Edit profile", edit_user_registration_path, :class => 'navbar-link' %> |
						<%= link_to "Logout", destroy_user_session_path, method: :delete, :class => 'navbar-link'  %>
					<% else %>|
						<%= link_to "Login", new_user_session_path, :class => 'navbar-link'  %>
					<% end %>
				</ul>
				<ul class="nav pull-left">
					<li><%= link_to "View Calendar", calendar_path %></li>
					<li><%= link_to "New Event", new_event_path %></li>
					<li><%= link_to "List Events", events_path %></li>
					<% if user_signed_in? %>
						<li class="signedIn"><%= link_to "Admin Help", adminHelp_path %></li>
					<% else %>
						<li class="signedOut"><%= link_to "Help", userHelp_path %></li>
						<li><%= link_to "Contact", contact_path %></li>
					<% end %>
					
				</ul>
			</div>
		</div>
	</div>

<% if notice %>
  <p class="alert alert-notice"><%= notice %></p>
<% end %>
<% if alert %>
  <p class="alert alert-error"><%= alert %></p>
<% end %>
<%= yield %>
</body>
</html>
